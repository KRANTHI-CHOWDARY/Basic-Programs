import java.util.*;
public class Fibonacci
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
		System.out.println("Enter the limit of the series: ");
		int n=sc.nextInt(),a=0,b=1;
		System.out.print("0 1 ");
		for(int i=0;i<n-2;i++)
		{
		    int c=a+b;
		    System.out.print(c+" ");
		    a=b;
		    b=c;
		}
	}
}
